<% include partials/header%>

  <center><h2>Output</h2></center>

     <div class="form-group">
    <label for="exampleFormControlTextarea3"><strong>Source Code</strong></label>
    <textarea class="form-control" id="exampleFormControlTextarea3" rows="15" name="source" required="1" placeholer="Source" wrap="soft"></textarea>
    </div>
    
    <div class="form-group">
    <label for="exampleFormControlTextarea3"><strong>Standard Input</strong></label>
    <textarea class="form-control" id="exampleFormControlTextarea3" rows="3" name="stdin" wrap="soft"></textarea>
    </div>
 

    <div>
    <select name="lang" class="mdb-select md-form">
     <option value="c">C</option>
      <option value="cpp">C++</option>
      <option value="java">Java</option>
      <option value="python2">Python</option>
    </select>
   </div>
    
    <br>
    <div>
   <input type="submit" class="btn btn-primary btn-md" value="Submit">
   </div>
   
   <div class="form-group">
    <label for="exampleFormControlTextarea3"><strong>Output</strong></label>
    <textarea class="form-control" id="exampleFormControlTextarea3" rows="3" name="output" wrap="soft"><%=body['output']%></textarea>
    </div>

<% include partials/footer%>